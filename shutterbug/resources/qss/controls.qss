#scrubbySlider {
    background-color: {{ bg_1 }};
    border-radius: 4px;

}
#scrubbySlider > QPushButton {
    background-color: transparent;
    outline: none;
    border: none;
    max-width: 10px;
    min-width: 10px;
    padding: 2px;
}

#scrubbySlider > QLineEdit {
    background-color: transparent;
    selection-background-color: {{ bg_3 }};
    border: none;
    padding-left: 0px;
    padding-right: 0px;
    text-align: center;
}
